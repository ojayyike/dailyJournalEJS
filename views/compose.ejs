<%- include('partials/header'); %>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for ="composeTitle">Title</label>
        <input type="text" class="form-control" name="composeTitleText" id="" autofill="false">
    </div>
    <div class="form-group">
        <label for="composePost">Post</label>
        <textarea class="form-control" name="composePostTextArea" rows="5" cols="30" autofill="false"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Publish</button>
</form>
<footer>
    <%- include('partials/footer'); %>
</footer>